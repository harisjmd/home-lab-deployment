pageInfo:
  title: CSCH Dashy
  navLinks:
    - title: GitHub
      path: https://github.com/harisjmd
    - title: Google
      path: https://www.google.com
appConfig:
  theme: colorful
  layout: auto
  iconSize: medium
  language: en
  defaultOpeningMethod: newtab
  statusCheck: true
  statusCheckInterval: 10
  customColors:
    colorful:
      primary: '#e8eae1'
      background: '#1c1c1c'
      background-darker: '#202122'
      info: '#04e4f4'
      success: '#20e253'
      warning: '#f6f000'
      error: '#fca016'
      danger: '#f80363'
      neutral: '#272f4d'
      white: '#fff'
      black: '#000'
      item-group-background: transparent
      medium-grey: '#5e6474'
      item-background: '#202122'
      item-background-hover: '#1c1c1c'
      transparent-70: rgba(0,0,0,0.701961)
      transparent-50: rgba(0,0,0,0.501961)
      transparent-30: rgba(0,0,0,0.301961)
      transparent-white-70: hsla(0,0%,100%,0.701961)
      transparent-white-50: hsla(0,0%,100%,0.501961)
      transparent-white-30: hsla(0,0%,100%,0.301961)
      heading-text-color: '#e8eae1'
      nav-link-text-color: '#e8eae1'
      nav-link-background-color: '#1c1c1c'
      nav-link-text-color-hover: '#e8eae1'
      nav-link-background-color-hover: rgba(96,125,139,0.2)
      nav-link-border-color: transparent
      nav-link-border-color-hover: '#e8eae1'
      nav-link-shadow: '1px 1px 2px #232323'
      nav-link-shadow-hover: '1px 1px 2px #232323'
      item-text-color: '#e8eae1'
      item-text-color-hover: '#e8eae1'
      item-group-outer-background: '#202122'
      item-group-heading-text-color: '#e8eae1'
      item-group-heading-text-color-hover: '#e8eae1'
      settings-text-color: '#e8eae1'
      settings-background: '#1c1c1c'
      config-settings-color: '#e8eae1'
      config-settings-background: '#202122'
      config-code-color: '#1c1c1c'
      config-code-background: '#fff'
      code-editor-color: '#000'
      code-editor-background: '#fff'
      widget-text-color: '#e8eae1'
      widget-background-color: '#202122'
      widget-accent-color: '#1c1c1c'
      widget-base-background: transparent
      interactive-editor-color: '#e8eae1'
